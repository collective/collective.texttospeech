<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:genericsetup="http://namespaces.zope.org/genericsetup"
    xmlns:i18n="http://namespaces.zope.org/i18n"
    i18n_domain="collective.texttospeech">

  <genericsetup:registerProfile
      name="default"
      title="collective.texttospeech"
      directory="profiles/default"
      description="Installs collective.texttospeech."
      provides="Products.GenericSetup.interfaces.EXTENSION"
      i18n:attributes="title; description"
      />

  <genericsetup:registerProfile
      name="uninstall"
      title="collective.texttospeech: Remoção do Pacote"
      directory="profiles/uninstall"
      description="Uninstalls collective.texttospeech."
      provides="Products.GenericSetup.interfaces.EXTENSION"
      i18n:attributes="title; description"
      />

  <!--include package=".upgrades" /-->

  <utility factory=".setuphandlers.HiddenProfiles" name="collective.texttospeech" />

</configure>
